             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Adressbuch"
             mc:Ignorable="d" 
             Background="White"
             d:DesignHeight="345" d:DesignWidth="610">
